{
  "consumer": {
    "name": "Event API Consumer"
  },
  "interactions": [
    {
      "description": "a request to retrieve events with type 'DetailsView'",
      "providerStates": [
        {
          "name": "at least one event of type eventType exists",
          "params": {
            "eventType": "DetailsView"
          }
        }
      ],
      "request": {
        "headers": {
          "Accept": "application/json",
          "Authorization": "Bearer SomeValidAuthToken"
        },
        "method": "GET",
        "path": "/events",
        "query": {
          "type": [
            "DetailsView"
          ]
        }
      },
      "response": {
        "body": [
          {
            "eventType": "DetailsView"
          }
        ],
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pactRust": {
      "version": "0.9.5"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "Event API"
  }
}